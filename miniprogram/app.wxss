page {
  background: #f5f5f5;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  font-family: -apple-system, BlinkMacSystemFont, PingFang SC, Noto Sans, Noto Sans CJK SC, Microsoft YaHei UI, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif, Segoe UI, Roboto, Helvetica Neue, Arial, "Apple Color Emoji", "Segoe UI Emoji", Segoe UI Symbol, "Noto Color Emoji";
}

.maskHeader {
  position: fixed;
  top: 0;
  width: 100%;
  height: 200rpx;
  z-index: 2;
  background: linear-gradient(180deg, #F5F5F5 0%, rgba(239, 239, 239, 0.772917) 58.12%, rgba(217, 217, 217, 0) 100%);
}

.todo-block {
  margin: 0rpx 20rpx;
}

.todo::before {
  content: '';
  position: absolute;
  margin-left: -40rpx;
  margin-top: 5rpx;
  height: 36rpx;
  width: 57rpx;
  background-size: cover;
  animation: show 2s forwards alternate-reverse infinite ease-in-out;
  background-image: url(data:image/svg+xml;base64,);
}

.done::before {
  content: '';
  position: absolute;
  margin-left: -40rpx;
  margin-top: 5rpx;
  height: 36rpx;
  width: 57rpx;
  background-size: cover;
  background-image: url(data:image/svg+xml;base64,);
}

.searchIcon {
  position: fixed;
  z-index: 3;
  border-radius: 50%;
  opacity: 0.6;
  backdrop-filter: blur(100rpx);
  border: 1rpx solid #00000020;
  background-size: cover;
  background-image: url(data:image/svg+xml;base64,);
}

.link {
  color: #0077e6;
  line-height: 80rpx;
  font-weight: 900;
  font-family:Noto Sans;
}

.link::before {
  content: 'ðŸ”—';
  opacity: 0.3;
  font-size: 27rpx;
}

.content .code {
  margin: 10rpx;
  white-space: pre;
  overflow: scroll;
  background-color: #f5f5f5;
  color: #00000080;
  padding: 30rpx;
  border-radius: 10rpx;
  font-family: source-code-pro, Monaco, Consolas, "Courier New", -apple-system, BlinkMacSystemFont, PingFang SC, Noto Sans, Noto Sans CJK SC, Microsoft YaHei UI, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif, Segoe UI, Roboto, Helvetica Neue, Arial, "Apple Color Emoji", "Segoe UI Emoji", Segoe UI Symbol, "Noto Color Emoji";
}

.tag-span {
  color: #0077e6;
  line-height: 80rpx;
  font-weight: 900;
  font-family:Noto Sans;
}

.img {
  max-width: 80%;
  min-height: 200rpx;
}

.halfDialog {
  display: flex;
  position: sticky;
  box-shadow: 0px 10px 10px #f5f5f5;
  top: 200rpx;
  transition: height 0.2s ease-in-out;
  overflow: hidden;
  flex-direction: column;
  z-index: 2;
  background: #ffffff;
  border-radius: 30rpx 30rpx 0rpx 0rpx;
  width: calc(100% - 60rpx);
  margin: 10rpx 30rpx;
  border-radius: 30rpx;
  box-shadow: 0 0 50rpx #00000008;
}

/* .showHalfDialog {
  animation: halfDialog 500ms 100ms forwards;
}

.closeHalfDialog {
  animation: halfDialog 500ms reverse forwards;
} */

.title {
  margin: 30rpx;
  color: #00000080;
}

.close {
  transform: rotate(45deg);
  transition: 500ms;
  position: absolute;
  margin-top: 25rpx;
  right: 60rpx;
  width: 50rpx;
  height: 50rpx;
  background-size: cover;
  background-image: url(data:image/svg+xml;base64,);
}

.open {
  transform: rotate(0deg);
  transition: 500ms;
}

.halfDialog textarea {
  width: calc(90% - 60rpx);
  border-radius: 20rpx;
  padding: 30rpx;
  background: #f5f5f5;
  margin: 0rpx 5%;
  margin-bottom: 20rpx;
}

.memos {
  z-index: 1;
}

.memoCard {
  /* opacity: 0;
  animation: show 300ms 300ms forwards; */
  white-space: pre-wrap;
  margin: 20rpx 30rpx;
  padding: 30rpx;
  background: #ffffff;
  font-size: 35rpx;
  /* border: 1rpx solid #e5e7eb; */
  border-radius: 20rpx;
}

.memoCard:last-child {
  margin-bottom: 40rpx;
}

.memoCard .time {
  letter-spacing: -1rpx;
  color: #cccccc;
  font-size: 30rpx;
  margin-right: 20rpx;
  letter-spacing: 2rpx;
}


.archivedbtn {
  /* position: sticky;
  bottom: 80rpx;
  box-shadow: 0 0 20px ; */
  opacity: 0.7;
  width: calc(100% - 110rpx);
  height: 100rpx;
  margin: 30rpx;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  padding-left: 50rpx;
  background-size: cover;
  background-image: url(data:image/svg+xml;base64,);
}

.none {
  margin: 30rpx;
  font-size: 30rpx;
  color: #00000050;
  text-align: center;
  opacity: 0;
  animation: show 300ms 500ms forwards;
}

.memoCard .status {
  border: 1rpx solid;
  color: #07C160;
  font-size: 25rpx;
  padding: 5rpx 10rpx;
  border-radius: 10rpx;
}

.memoCard .content {
  word-break: break-word;
  margin: 10rpx;
  margin-top: 20rpx;
  font-size: 35rpx;
  letter-spacing: 2rpx;
}

@keyframes show {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

@keyframes cardshow {
  from {
    transform: rotate3d(0, 1, 0, 30deg);
  }

  to {
    transform: rotate3d(1, 0, 0, 0deg);
  }
}

@keyframes halfDialog {
  from {
    height: 100rpx;
  }

  to {
    height: 600rpx;
  }
}

@import 'app-wxa-auto-dark.wxss'