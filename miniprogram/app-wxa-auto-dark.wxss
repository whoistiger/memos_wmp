@media (prefers-color-scheme: dark) {
  page {
    background: rgb(17, 17, 17);
  }

  .maskHeader {
    background: linear-gradient(180deg, #111111 0%, rgba(0, 0, 0, 0.773) 58.12%, rgba(8, 8, 8, 0) 100%);
  }

  .searchIcon {
    border: rgb(0, 0, 0);
  }

  textarea {
    color: #ffffff80;
  }

  .memoCard .content {
    color: #ffffff80;
  }

  input {
    color: #ffffff80;
  }

  .link {
    color: rgb(48, 134, 247);
  }

  .content .code {
    background-color: rgb(27, 27, 27);
    color: rgba(255, 255, 255, 0.5019607843137255);
  }

  .tag-span {
    color: rgb(48, 134, 247);
  }

  .halfDialog {
    background: rgb(24, 24, 24);
    box-shadow: rgb(0, 0, 0);
  }

  .halfDialog .title {
    color: rgba(255, 255, 255, 0.5019607843137255);
  }

  .halfDialog textarea {
    background: rgb(17, 17, 17);
  }

  .memoCard {
    background: rgb(24, 24, 24);
  }

  .memoCard .time {
    color: rgb(55, 55, 55);
  }

  .none {
    color: rgba(255, 255, 255, 0.3137254901960784);
  }

  .memoCard .status {
    color: rgb(0, 105, 12);
  }

  .done::before {
    opacity: 0.8;
    background-image: url(data:image/svg+xml;base64,);
  }

  .todo::before {
    background-image: url(data:image/svg+xml;base64,);
  }

  @keyframes show {
    from {
      opacity: 0;
    }

    to {
      opacity: 1;
    }
  }

  @keyframes cardshow {
    from {
      transform: rotate3d(0, 1, 0, 30deg);
    }

    to {
      transform: rotate3d(1, 0, 0, 0deg);
    }
  }

  @keyframes halfDialog {
    from {
      height: 100rpx;
    }

    to {
      height: 600rpx;
    }
  }
}